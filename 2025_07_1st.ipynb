    "def solution(n):\n",
    "    target = bin(n).count('1')\n",
    "    cur = n + 1\n",
    "    while True:\n",
    "        if bin(cur).count('1') == target:\n",
    "            return cur\n",
    "        cur += 1\n",
    "        "
